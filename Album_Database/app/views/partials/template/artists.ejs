<div class="container">
        <div class="row">
            <div class="col-sm">
                <h2>Artist ID</h1>
            </div>
            <div class="col-sm">
                <h2>Artist Name</h1>
            </div>
        </div>
    </div>
<% if (artists.length > 0) { %>
<% for (i=0; i< artists.length; i++) { %>
    <div class="container">
        <div class="row dataRow">
            <div class="col-sm">
                <p><%= artists[i].id %></p>
            </div>
            <div class="col-sm">
                <p><%= artists[i].artist_name %></p>
            </div>
        </div>
    </div>

<br>
<% } %>
<% } %>